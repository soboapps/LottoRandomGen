<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.soboapps.lottorandomgen" >

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name=".MainActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <activity
            android:label="@string/app_name"
            android:name=".GenerateRandomPickThree"
            android:exported="false">
            <intent-filter>
                <action android:name="com.soboapps.lottorandomgen.GENERATERANDOMPICKTHREE" />
                <category android:name="android.intent.category.DEFAULT" />
            </intent-filter>
        </activity>

        <activity
            android:label="@string/app_name"
            android:name=".GenerateRandomPickFour"
            android:exported="false">
            <intent-filter>
                <action android:name="com.soboapps.lottorandomgen.GENERATERANDOMPICKFOUR" />
                <category android:name="android.intent.category.DEFAULT" />
            </intent-filter>
        </activity>

        <activity
            android:label="@string/app_name"
            android:name=".GenerateRandomPickFive"
            android:exported="false">
            <intent-filter>
                <action android:name="com.soboapps.lottorandomgen.GENERATERANDOMPICKFIVE" />
                <category android:name="android.intent.category.DEFAULT" />
            </intent-filter>
        </activity>
    </application>

</manifest>
